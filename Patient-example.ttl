# - resource -------------------------------------------------------------------

<https://lishathomas19.github.io/lisha-demo-ig-site/Patient/example> a fhir:Patient ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "example"] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div [ fhir:v "<div xmlns=\"http://www.w3.org/1999/xhtml\">Example patient instance used in the Lisha Demo IG.</div>"^^rdf:XMLLiteral ]
  ] ; # 
  fhir:name ( [
     fhir:family [ fhir:v "Test" ] ;
     fhir:given ( [ fhir:v "Demo" ] )
  ] ) ; # 
  fhir:gender [ fhir:v "female"] . # 

